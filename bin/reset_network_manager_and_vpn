#!/bin/bash
# reset intratubes
if [ $# -eq 0 ]
then
  server=1583
else
  server=$1
fi
sudo killall -q openvpn
sudo service network-manager stop
sleep 1
sudo service network-manager start
sleep 1
sudo openvpn --config /etc/openvpn/ovpn_tcp/us"${server}".nordvpn.com.tcp.ovpn --auth-user-pass ~/.ovpnuserpass --daemon --writepid ~/.openvpn_pid
