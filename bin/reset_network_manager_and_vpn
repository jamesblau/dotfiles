#!/bin/bash
# reset intratubes
if [ $# -eq 0 ]
then
  server=1583
else
  server=$1
fi
sudo killall -q openvpn
sudo service network-manager stop
sleep 0.5
sudo service network-manager start
sleep 0.5
sudo openvpn --config /etc/openvpn/ovpn_udp/us"${server}".nordvpn.com.udp.ovpn --auth-user-pass ~/.ovpnuserpass --daemon
