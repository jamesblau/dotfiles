" vim: sw=2 ts=2 sts=2 expandtab ft=vim
let g:james_minimal_plugins=1
let g:james_pager=1
let g:page_icon_instance = 'ยง'
let g:page_icon_redirect = '>ยง'
let g:page_icon_pipe = '|ยง'
source ~/.vimrc
setl shortmess=I
setl scrollback=-1
setl scrolloff=999
setl signcolumn=no
setl modified
setl nomodifiable
setl invhlsearch
setl clipboard+=unnamedplus
setl laststatus=0
" setl termguicolors
nmap i <Nop>
nmap I <Nop>
nmap a <Nop>
nmap A <Nop>
nmap o <Nop>
nmap O <Nop>
nmap q :q<CR>
nmap <C-C> :q<CR>
" au TermEnter * exec normal G
