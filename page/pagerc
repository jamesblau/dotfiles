" vim: sw=2 ts=2 sts=2 expandtab ft=vim
let g:james_minimal_plugins=1
setl scrollback=-1
setl scrolloff=999
setl signcolumn=no
setl modified
setl nomodifiable
setl invhlsearch
setl clipboard+=unnamedplus
nmap i <Nop>
nmap I <Nop>
nmap a <Nop>
nmap A <Nop>
nmap o <Nop>
nmap O <Nop>
nmap q :q<CR>
nmap <C-C> :q<CR>
